{
    "name": "Ridge Linear Regression",
    "module": "sklearn.linear_model.Ridge",
    "parameters": {
        "alpha": [
            "float",
            "array:float"
        ],
        "fit_intercept": "bool",
        "copy_X": "bool",
        "max_iter": "int",
        "tol": "float",
        "solver": [
            "auto",
            "svd",
            "cholesky",
            "lsqr",
            "sag",
            "lbfgs"
        ],
        "positive": "bool",
        "random_state": "int"
    },
    "preprocessing": [
        {
            "name": "Polynomial Feature",
            "module": "sklearn.preprocessing.PolynomialFeatures",
            "parameters": {
                "degree": "int"
            }
        }
    ],
    "hyperparameter_exploration": [
        {
            "_pre_0_degree": [
                2,
                4,
                8
            ]
        }
    ]
}